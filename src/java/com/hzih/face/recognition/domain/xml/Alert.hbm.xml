<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="center_alert" name="com.hzih.face.recognition.domain.Alert">
    <id access="field" name="id" type="java.lang.Long" column="id">
      <generator class="sequence"/>
    </id>
    <property name="featureId" column="action" access="field" type="java.lang.String"/>
    <property name="terminalId" column="terminalId" access="field" type="java.lang.String"/>
    <property name="taskId" column="taskId" access="field" type="java.lang.String"/>
    <property name="personInfo" column="personInfo" access="field" type="java.lang.String"/>
    <property name="ipcFilePath" column="ipcFilePath" access="field" type="java.lang.String"/>
    <property name="personFilePath" column="personFilePath" access="field" type="java.lang.String"/>
    <property name="alertLevel" column="alertLevel" access="field" type="java.lang.String"/>
    <property name="score" column="score" access="field" type="java.lang.Integer"/>
    <property name="alertTime" column="alertTime" access="field" type="java.util.Date"/>
  </class>
</hibernate-mapping>
