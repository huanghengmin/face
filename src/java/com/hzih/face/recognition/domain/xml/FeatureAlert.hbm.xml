<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="center_feature_alert" name="com.hzih.face.recognition.domain.FeatureAlert">
    <id access="field" name="id" type="java.lang.String" column="id">
      <generator class="assigned"/>
    </id>
    <property name="action" column="action" access="field" type="java.lang.String"/>
    <property name="targetDB" column="targetDB" access="field" type="java.lang.String"/>
    <property name="alertLevel" column="alertLevel" access="field" type="java.lang.String"/>
  </class>
</hibernate-mapping>
